{
  "name": "storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-storybook": "build-storybook",
    "build-cesium-umd": "node --max_old_space_size=8196 ./node_modules/@vue/cli-service/bin/vue-cli-service build --target lib --name webclient-vue-cesium cesium/src/main.js",
    "build-cesium-cdn": "node --max_old_space_size=8196 ./node_modules/@vue/cli-service/bin/vue-cli-service build --target lib --name webclient-vue-cesium cesium/src/cdn/main.js",
    "build-mapboxgl-umd": "node --max_old_space_size=8196 ./node_modules/@vue/cli-service/bin/vue-cli-service build --target lib --name webclient-vue-mapboxgl mapboxgl/src/main.js",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.2.0",
    "@mapgis/cesium": "^1.59.0",
    "@mapgis/mapbox-gl": "^1.9.7",
    "@mapgis/mapbox-gl-compare": "^0.4.0",
    "@mapgis/mapbox-gl-draw-radius": "^1.0.0",
    "@mapgis/webclient-es6-service": "^1.0.3",
    "@mapgis/webclient-store": "^1.0.14",
    "@turf/turf": "^6.3.0",
    "ant-design-vue": "^1.7.3",
    "core-js": "^3.6.5",
    "echarts": "^5.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "fast-xml-parser": "^3.19.0",
    "http-proxy-middleware": "^1.0.6",
    "js-cookie": "^2.2.1",
    "mapbox-gl-draw-circle": "^1.1.2",
    "mapbox-gl-draw-rectangle-mode": "^1.0.4",
    "mapv": "^2.0.62",
    "node-sql-parser": "^3.1.0",
    "typescript": "^4.1.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.22.4",
    "vue-loader": "^15.9.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/vue": "^6.2.9",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.15.0",
    "eslint-plugin-vue": "^7.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
